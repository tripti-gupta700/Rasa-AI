{
  "name": "rasa-ai-monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "Rasa AI: An intelligent Ayurvedic health assistant.",
  "type": "module",
  "scripts": {
    "backend": "npm run dev --prefix backend",
    "frontend": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "install:all": "npm install && npm install --prefix backend"
  },
  "dependencies": {
    "@google/genai": "^0.15.0",
    "framer-motion": "^11.0.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  }
}